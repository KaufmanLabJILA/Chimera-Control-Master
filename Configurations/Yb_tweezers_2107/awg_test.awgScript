% start by resetting synthesizer



reset





%setsteps 0 100 0 2*rbtime

%ampgauss 0 0 2*rbtime 2*rbtime/3 1 0 40 

%freqlin 0 0 2*rbtime 99.8 99.8 1 0





%setsteps 0 100 2*rbtime 4*rbtime

%ampgauss 0 2*rbtime 4*rbtime 2*rbtime/3 -1 40 0

%freqlin 0 2*rbtime 4*rbtime 99.8 99.8 0 0



%setsteps 0 time/100 5*rbtime 5*rbtime+time

%ampgauss 0 5*rbtime 5*rbtime+time time/3 1 0 40 

%freqlin 0 5*rbtime 5*rbtime+time 99.8 99.8 0 0





%setsteps 0 time/100 5*rbtime+time 5*rbtime+2*time

%ampgauss 0 5*rbtime+time 5*rbtime+2*time time/3 -1 40 0

%freqlin 0 5*rbtime+time 5*rbtime+2*time 99.8 99.8 0 0



setsteps 0 rb4time/100 0 rb4time

ampgauss 0 0 rb4time rb4time/3 1 0 30

freqlin 0 0 rb4time rbcarrier+rb4freq_pulsed rbcarrier+rb4freq_pulsed 1 0



setsteps 0 rb4time/100 rb4time 2*rb4time

ampgauss 0 rb4time 2*rb4time rb4time/3 -1 30 0

freqlin 0 rb4time 2*rb4time rbcarrier+rb4freq_pulsed rbcarrier+rb4freq_pulsed 0 0



setsingle 0 2*rb4time 0 0 0 0 



program 0





setsteps 1 rb2time/100 0 rb2time

ampgauss 1 0 rb2time rb2time/3 1 0 70 

freqlin 1 0 rb2time rbcarrier+rb2freq_pulsed rbcarrier+rb2freq_pulsed 1 0



setsteps 1 rb2time/100 rb2time 2*rb2time

ampgauss 1 rb2time 2*rb2time rb2time/3 -1 70 0

freqlin 1 rb2time 2*rb2time rbcarrier+rb2freq_pulsed rbcarrier+rb2freq_pulsed 0 0



setsingle 1 2*rb2time 0 0 0 0 



program 1



setsteps 2 rbtime/100 0 rbtime

ampgauss 2 0 rbtime rbtime/3 1 0 30

freqlin 2 0 rbtime freq freq  1 0



setsteps 2 rbtime/100 rbtime 2*rbtime

ampgauss 2 rbtime 2*rbtime rbtime/3 -1 30 0

freqlin 2 rbtime 2*rbtime freq  freq  0 0



setsingle 2 2*rbtime 0 0 0 0 



program 2



setsteps 3 rb3time/100 0 rb3time

ampgauss 3 0 rb3time rb3time/3 1 0 70 

freqlin 3 0 rb3time rbcarrier+rb3freq_pulsed rbcarrier+rb3freq_pulsed 1 0



setsteps 3 rb3time/100 rb3time 2*rb3time

ampgauss 3 rb3time 2*rb3time rb3time/3 -1 70 0

freqlin 3 rb3time 2*rb3time rbcarrier+rb3freq_pulsed rbcarrier+rb3freq_pulsed 0 0



setsingle 3 2*rb3time 0 0 0 0 



program 3



% Software trigger for testing



%trigger



