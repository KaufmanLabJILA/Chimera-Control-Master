% start by resetting synthesizer



reset





%setsteps 0 100 0 2*rbtime

%ampgauss 0 0 2*rbtime 2*rbtime/3 1 0 40 

%freqlin 0 0 2*rbtime 99.8 99.8 1 0





%setsteps 0 100 2*rbtime 4*rbtime

%ampgauss 0 2*rbtime 4*rbtime 2*rbtime/3 -1 40 0

%freqlin 0 2*rbtime 4*rbtime 99.8 99.8 0 0



%setsteps 0 time/100 5*rbtime 5*rbtime+time

%ampgauss 0 5*rbtime 5*rbtime+time time/3 1 0 40 

%freqlin 0 5*rbtime 5*rbtime+time 99.8 99.8 0 0





%setsteps 0 time/100 5*rbtime+time 5*rbtime+2*time

%ampgauss 0 5*rbtime+time 5*rbtime+2*time time/3 -1 40 0

%freqlin 0 5*rbtime+time 5*rbtime+2*time 99.8 99.8 0 0



%setsingle 0 0 100 clockfreq 0 0 

%setsteps 0 clocktime/20 0 clocktime


%ampgauss 0 0 clocktime clocktime/3 1 0 100

%freqlin 0 0 clocktime clockfreq-clockdet  clockfreq  0 0

%setsteps 0 clocktime/20 clocktime 2*clocktime

%ampgauss 0 clocktime 2*clocktime clocktime/3 -1 100 0

%freqlin 0 clocktime 2*clocktime clockfreq clockfreq+clockdet 0 0

%
setsingle 0 0 100 clockfreq 0 0
%setsingle 0 clocktime 100 clockfreq 1 313.43 
%setsingle 0 3*clocktime 100 clockfreq 1 104.48 
%setsingle 0 4*clocktime 100 clockfreq 1 0
%setsingle 0 5*clocktime 0 clockfreq 0 0
setsingle 0 clocktime 0 clockfreq 0 0
%setsingle 0 0 0 clockfreq 0 0 



program 0


setsingle 1 0 100 100 1 0 

setsingle 1 2*rb2time 0 100 0 0 

program 1



%setsingle 2 0 100 90 0 0 

%setsingle 2 100*1000 100 0 0 0 

setsteps 2 rb2time/50 0 rb2time

ampgauss 2 0 rb2time rb2time/3 1 0 100 

freqlin 2 0 rb2time 100+rb2det 100+rb2det 1 0


setsteps 2 rb2time/50 rb2time 2*rb2time

ampgauss 2 rb2time 2*rb2time rb2time/3 -1 100 0

freqlin 2 rb2time 2*rb2time 100+rb2det 100+rb2det 0 0

setsingle 2 2*rb2time 0 100 0 0 

program 2


setsteps 3 2*rb3time/30 0 2*rb3time

ampgauss 3 0 2*rb3time 2*rb3time/3 1 0 100 

freqlin 3 0 2*rb3time rb1freq+rb3det rb1freq+rb3det 1 0


setsteps 3 2*rb3time/30 2*rb3time 4*rb3time

ampgauss 3 2*rb3time 4*rb3time 2*rb3time/3 -1 100 0

freqlin 3 2*rb3time 4*rb3time rb1freq+rb3det rb1freq+rb3det 0 0

setsingle 3 4*rb3time 0 100 0 0 


program 3



% Software trigger for testing



%trigger
