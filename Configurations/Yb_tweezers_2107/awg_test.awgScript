% start by resetting synthesizer



reset





%setsteps 0 100 0 2*rbtime

%ampgauss 0 0 2*rbtime 2*rbtime/3 1 0 40 

%freqlin 0 0 2*rbtime 99.8 99.8 1 0





%setsteps 0 100 2*rbtime 4*rbtime

%ampgauss 0 2*rbtime 4*rbtime 2*rbtime/3 -1 40 0

%freqlin 0 2*rbtime 4*rbtime 99.8 99.8 0 0



%setsteps 0 time/100 5*rbtime 5*rbtime+time

%ampgauss 0 5*rbtime 5*rbtime+time time/3 1 0 40 

%freqlin 0 5*rbtime 5*rbtime+time 99.8 99.8 0 0





%setsteps 0 time/100 5*rbtime+time 5*rbtime+2*time

%ampgauss 0 5*rbtime+time 5*rbtime+2*time time/3 -1 40 0

%freqlin 0 5*rbtime+time 5*rbtime+2*time 99.8 99.8 0 0



%setsingle 0 0 100 clockfreq 0 0 

%setsteps 0 clocktime/35 0 clocktime

%ampgauss 0 0 clocktime clocktime/2.5 1 0 2

%freqlin 0 0 clocktime clockfreq  clockfreq  0 0

%setsteps 0 clocktime/35 clocktime clocktime*2

%ampgauss 0 clocktime 2*clocktime clocktime/2.5 -1 2 0

%freqlin 0 clocktime 2*clocktime clockfreq clockfreq 0 0

%
%setsingle 0 0 100 clockfreq 0 0
setsingle 0 2*clocktime 0 clockfreq 0 0


program 0


setsteps 1 rbtime/15 0 rbtime

ampgauss 1 0 rbtime rbtime/2.5 1 0 80 

freqlin 1 0 rbtime rbfreq rbfreq  1 0


setsteps 1 rbtime/15 rbtime 2*rbtime

ampgauss 1 rbtime 2*rbtime rbtime/2.5 -1 80 0

freqlin 1 rbtime 2*rbtime rbfreq rbfreq 0 0

setsingle 1 2*rbtime 0 100 0 0 

program 1



%setsingle 2 0 100 90 0 0 

%setsingle 2 100*1000 100 0 0 0 

setsteps 2 rb2time/15 0 rb2time

ampgauss 2 0 rb2time rb2time/2.5 1 0 80 

freqlin 2 0 rb2time rb2freq rb2freq  1 0


setsteps 2 rb2time/15 rb2time 2*rb2time

ampgauss 2 rb2time 2*rb2time rb2time/2.5 -1 80 0

freqlin 2 rb2time 2*rb2time rb2freq rb2freq 0 0

setsingle 2 2*rb2time 0 100 0 0 

program 2

%setsingle 2 0 100 90 0 0 

%setsingle 2 100*1000 100 0 0 0 

setsteps 3 rb3time/15 0 rb3time

ampgauss 3 0 rb3time rb3time/2.5 1 0 50 

freqlin 3 0 rb3time rb3freq rb3freq  1 0


setsteps 3 rb3time/15 rb3time 2*rb3time

ampgauss 3 rb3time 2*rb3time rb3time/2.5 -1 50 0

freqlin 3 rb3time 2*rb3time rb3freq rb3freq 0 0

setsingle 3 2*rb3time 0 100 0 0 


program 3



% Software trigger for testing



%trigger
