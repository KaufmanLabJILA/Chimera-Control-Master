def rydberg_multipulse(ryd_time):
  % insert commands here...
  on: uvttl
  off: pi407
  on: latticehold
  pulseon: trigger .001
  pulseon: tektronixtrig .001
  t += .001
  on: latblink
  t += .0001
  repeat: 4
  pulseon: awgtrig .0001
  t += .08/1000
  t += ryd_time/1000
  t += .1/1000
  end
  t += .0006
  off: latblink
  t += .01 
  %t += time407/1000
  %off: uvttl
  on: pi407
  off: latticehold

