def rydberg_pulse(ryd_time):
  % insert commands here...
  on: uvttl
  off: pi407
  on: latticehold
  t += .001
  pulseon: awgtrig .0001
  t += (uvblinkdelay+1.08)/1000
  on: latblink
  t += ryd_time/1000
  t += .0003
  off: latblink 
  t += time407/1000
  off: uvttl
  on: pi407
  off: latticehold
