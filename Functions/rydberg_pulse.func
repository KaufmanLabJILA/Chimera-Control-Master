def rydberg_pulse(ryd_time):
  % insert commands here...
  on: uvttl
  off: pi407
  on: latticehold
  t += .001
  pulseon: awgtrig .0001
  t += (.59+.08)/1000
  on: latblink
  t += ryd_time/1000
  t += .0003
  off: latblink 
  t += .001
  off: uvttl
  off: latticehold