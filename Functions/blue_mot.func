def blue_mot(loadTime):


  dac: blue3dpow 0.3
  on: red3d
  on: blue3d
  on: rp707
  on: rp679

  on: slower
  dac: x1coil bluecoilx
  dac: y1coil bluecoily
  dac: y2coil bluecoily

  dac: z1coil bluecoilz
  dac: motcoils 4


  t += loadTime

  
  %Drop power for additional cooling
  dac: blue3dpow .03
  t += 5
  dac: blue3dpow 0.3
  off: blue3d
  off: rp707
  off: rp679
  t += 0.01

